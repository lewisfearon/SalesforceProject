<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Vat</fullName>
    <active>true</active>
    <errorConditionFormula>VAT_Rate__c &lt; 0.1  ||  VAT_Rate__c &gt; 1</errorConditionFormula>
    <errorDisplayField>VAT_Rate__c</errorDisplayField>
    <errorMessage>VAT rate cannot be above 100% or be below 0. Please enter the correct VAT rate.</errorMessage>
</ValidationRule>
